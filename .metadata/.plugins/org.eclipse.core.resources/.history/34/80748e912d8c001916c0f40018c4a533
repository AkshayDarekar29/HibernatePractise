package beans;

public class SEmployee extends Employee {
	String tool;
	
	public SEmployee(int id, String name, int salary, String tool) {
		super(id, name, salary);
		this.tool = tool;
	}
	
	public String getTool() {
		return tool;
	}

	public void setTool(String tool) {
		this.tool = tool;
	}

}
