<hibernate-mapping>
	<class name="beans.Course" table="Course">
		<id name="cId"/>
		<property name="cName"/>
		<property name="fee"/>
		<set name="faculty" table="facultyCourses">
			<key column="cId"></key>
			<many-to-many class="beans.Faculty"></many-to-many>
		</set>
	</class>
</hibernate-mapping>