package beans;

public class Movies {
	int mid;
	String mName;
	public Movies(int mid, String mName) {
		super();
		this.mid = mid;
		this.mName = mName;
	}
	public int getMid() {
		return mid;
	}
	public void setMid(int mid) {
		this.mid = mid;
	}
	public String getmName() {
		return mName;
	}
	public void setmName(String mName) {
		this.mName = mName;
	}

	
}
